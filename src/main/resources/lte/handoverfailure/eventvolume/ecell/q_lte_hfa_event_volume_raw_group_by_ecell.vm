##
## LTE Handover Failure eCell Event Volume Analysis (using raw tables)
##
## Velocity Parameter: 
##		
## Named PreparedStatement Parameter:
##	   	$starttime 		start time
## 		$endtime 		end time
##		$interval 		interval for different timerange
##     	:dateFrom   	starting time
##     	:dateTo     	ending time
##
#set($columnsToSelect=["DATETIME_ID", "EVENT_ID"])
#set($columnsToSelectforImpactedIMSI=["DATETIME_ID", "IMSI"])
#set($groupTable="GROUP_TYPE_E_RAT_VEND_HIER321")
#set($groupnameColumn="GROUP_NAME")
#set($joinKeys=["HIER321_ID"])

#DEFINE_CONSTANTS()

begin

select
	dateformat(err.TS, 'yyyy-mm-dd hh:mm:ss.S') as 'TIME',
	isnull(sum (case when err.EVENT_ID = 4111 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_PREP_X2_IN,       
    isnull(sum (case when err.EVENT_ID = 4110 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_PREP_X2_OUT,    
    isnull(sum (case when err.EVENT_ID = 4113 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_EXEC_X2_IN,    
    isnull(sum (case when err.EVENT_ID = 4112 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_EXEC_X2_OUT,    
    isnull(sum (case when err.EVENT_ID = 4103 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_PREP_S1_IN,
    isnull(sum (case when err.EVENT_ID = 4102 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_PREP_S1_OUT, 
    isnull(sum (case when err.EVENT_ID = 4105 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_EXEC_S1_IN,
    isnull(sum (case when err.EVENT_ID = 4104 then isnull(err.FAILURES, 0) end),0) as INTERNAL_PROC_HO_EXEC_S1_OUT,
	isnull(max(imsiTable.IMPACTED_SUBSCRIBERS), 0) as 'Impacted Subscribers'
	
from
	(
		select
			tmp_st,
			EVENT_ID,
			isnull(count(*), 0) as FAILURES
		from
			#REPLACE_RAW_VIEW_WITH_RAW_TABLES_AND_FILTER_COLUMNS_WITH_TAC_EXCLUSION_SPECIFY_COLUMNS_GROUPS($TECH_PACK_LIST.getAllRawTables() "rawview" $columnsToSelect)
			,#GET_CHART_TIMES($starttime $endtime $interval)
		where        
			DATETIME_ID >= tmp_st
			and DATETIME_ID < tmp_et
		group by
			tmp_st,
			EVENT_ID
	) as err ( TS, EVENT_ID, FAILURES)
	full outer join
	(
		select
			tmp_st,
			count(distinct(IMSI))
		from
			#REPLACE_RAW_VIEW_WITH_RAW_TABLES_AND_FILTER_COLUMNS_WITH_TAC_EXCLUSION_SPECIFY_COLUMNS_GROUPS($TECH_PACK_LIST.getAllRawTables() "rawview" $columnsToSelectforImpactedIMSI)
			,#GET_CHART_TIMES($starttime $endtime $interval)
		where
			DATETIME_ID >= tmp_st
			and DATETIME_ID < tmp_et
		group by
			tmp_st
	) as imsiTable(TS, IMPACTED_SUBSCRIBERS)
	on
		err.TS = imsiTable.TS
where
	err.TS is not null
group by
	dateformat(err.TS, 'yyyy-mm-dd hh:mm:ss.S')
order by
	dateformat(err.TS, 'yyyy-mm-dd hh:mm:ss.S')
	
end